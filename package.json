{
  "name": "nextjs",
  "version": "0.4.0",
  "private": true,
  "scripts": {
    "d": "rm -rf build && cross-env NODE_ENV=development tsc-watch -p tsconfig.server.json --onSuccess \"node build/index.js\"",
    "p": "cross-env NODE_ENV=production node index.js",
    "build": "rm -rf .next && rm -rf build && tsc -p tsconfig.server.json && next build && mv .next build/.next && cp package.json build && cp -r public build/public",
    "start": "cd build && NODE_ENV=production node index.js",
    "start:development": "rm -rf build && cross-env NODE_ENV=development tsc-watch -p tsconfig.server.json --onSuccess \"node build/index.js\"",
    "start:staging": "rm -rf build && cross-env NODE_ENV=staging tsc-watch -p tsconfig.server.json --onSuccess \"node build/index.js\"",
    "start:production": "rm -rf build && cross-env NODE_ENV=production tsc-watch -p tsconfig.server.json --onSuccess \"node build/index.js\""
  },
  "main": "server/index.js",
  "description": "Next.js with Express boilerplate and Mongoose CRUD support",
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@hapi/joi": "^16.1.8",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.0.1",
    "@types/express-session": "^1.15.16",
    "@types/hapi__joi": "^16.0.3",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/lru-cache": "^5.1.0",
    "@types/mongoose": "^5.5.32",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.12.14",
    "@zeit/next-sass": "^1.0.1",
    "add": "^2.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-mongo": "^3.2.0",
    "cross-env": "^6.0.3",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "http-proxy-middleware": "^0.20.0",
    "i": "^0.3.6",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.1",
    "lru-cache": "^5.1.1",
    "mongoose": "^5.7.13",
    "morgan": "^1.9.1",
    "next": "9.1.4",
    "next-compose-plugins": "^2.2.0",
    "next-i18next": "^2.1.1",
    "next-images": "^1.2.0",
    "next-optimized-images": "^2.5.4",
    "next-progressbar": "^1.0.0",
    "next-react-svg": "^1.1.0",
    "next-redux-wrapper": "^4.0.1",
    "next-server": "^9.0.5",
    "next-size": "^2.1.0",
    "npm": "^6.13.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "tsc-watch": "^4.0.0",
